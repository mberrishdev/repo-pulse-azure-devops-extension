{
  "manifestVersion": 1,
  "id": "repo-pulse",
  "name": "Repo Pulse",
  "publisher": "mberrishdev",
  "version": "0.0.149",
  "description": "A modern Azure DevOps extension built with React and TypeScript that provides enhanced repository and pull request management capabilities.",
  "public": true,
  "categories": ["Azure Repos"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": ["api-version/3.0"],
  "scopes": ["vso.code", "vso.code_write", "vso.build", "vso.project", "vso.build_execute"],
  "icons": {
    "default": "static/images/logo.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "contributions": [
    {
      "id": "home-page-overview",
      "type": "ms.vss-web.hub",
      "targets": ["ms.vss-web.home-hub-group"],
      "properties": {
        "name": "Repo Pulse",
        "uri": "dist/home/home.html",
        "order": 1,
        "supportsMobile": true
      }
    }
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "overview.md",
      "addressable": true
    }
  ]
}
